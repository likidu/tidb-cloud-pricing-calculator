{
  "version": 2,
  "installCommand": "pnpm i",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/login", "dest": "/index.html" },
    {
      "src": "/(.*)",
      "has": [
        { "type": "header", "key": "accept", "value": ".*text/html.*" }
      ],
      "dest": "/api/protect"
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
